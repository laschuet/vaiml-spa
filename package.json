{
  "name": "vaipp-spa",
  "version": "1.0.0",
  "description": "Visual analytics in participatory processes - Single page application",
  "author": "Lars SchÃ¼tz (https://github.com/laschuet)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "node dev.js",
    "dist": "node dist.js",
    "lint": "eslint --ext .jsx --ext .js .",
    "lint-fix": "eslint --fix --ext .jsx --ext .js .",
    "prettier": "prettier --single-quote --trailing-comma all \"src/**/*.{js,jsx}\"",
    "prettier-fix": "prettier --single-quote --trailing-comma all --write \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laschuet/vaipp-spa.git"
  },
  "bugs": {
    "url": "https://github.com/laschuet/vaipp-spa/issues"
  },
  "homepage": "https://github.com/laschuet/vaipp-spa",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-fbjs-opensource": "^2.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@primer/css": "^14.4.0",
    "@primer/octicons-react": "^10.0.0",
    "axios": "^0.19.2",
    "camelcase-keys": "^6.2.2",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.6.5",
    "history": "^5.0.0",
    "lodash-es": "^4.17.15",
    "normalizr": "^3.6.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "modules": false,
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "browserslist": [
    "last 2 versions",
    "safari > 8",
    "not ie < 11"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "fbjs-opensource",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier",
      "react",
      "react-hooks"
    ],
    "rules": {
      "babel/no-await-in-loop": "off",
      "no-await-in-loop": "error",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "prettier/prettier": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "**/node_modules",
    "dist/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
