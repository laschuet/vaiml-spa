{
  "name": "vaiml-spa",
  "version": "1.0.0",
  "description": "Visual analytics for interpretable machine learning - Single page application",
  "author": "Lars SchÃ¼tz (https://github.com/laschuet)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "node dev.js",
    "dist": "node dist.js",
    "lint": "eslint --ext .jsx --ext .js .",
    "lint-fix": "eslint --fix --ext .jsx --ext .js .",
    "prettier": "prettier --single-quote --trailing-comma all \"src/**/*.{js,jsx}\"",
    "prettier-fix": "prettier --single-quote --trailing-comma all --write \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laschuet/vaiml-spa.git"
  },
  "bugs": {
    "url": "https://github.com/laschuet/vaiml-spa/issues"
  },
  "homepage": "https://github.com/laschuet/vaiml-spa",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.0.1",
    "eslint-config-fbjs-opensource": "^2.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.12.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@githubprimer/octicons-react": "^8.5.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@primer/css": "^12.4.1",
    "axios": "^0.19.0",
    "camelcase-keys": "^6.0.0",
    "connected-react-router": "^6.5.2",
    "core-js": "^3.1.4",
    "history": "^4.9.0",
    "lodash-es": "^4.17.15",
    "normalizr": "^3.4.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.8",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "reselect": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "modules": false,
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "browserslist": [
    "last 2 versions",
    "safari > 8",
    "not ie < 11"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "fbjs-opensource",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier",
      "react",
      "react-hooks"
    ],
    "rules": {
      "babel/no-await-in-loop": "off",
      "no-await-in-loop": "error",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "prettier/prettier": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "**/node_modules",
    "dist/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
